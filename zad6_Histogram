#include "pch.h"
#include <iostream>
#include <stdlib.h>

int main()
{
	int tab[10];

	for (int i = 0; i < 10; i++)
	{
		scanf_s("%d", &tab[i]);
	}
	
	for (int i = 0; i < 10; i++)
	{
		for (int j = 0; j < 10; j++)
		{
			if (tab[j] > i )
				printf("# ");
			else
				printf("  ");
		}
		printf("\n");
	}
	
}

